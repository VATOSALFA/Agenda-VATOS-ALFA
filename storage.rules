
rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // Permite la lectura p√∫blica de todos los archivos
    match /{allPaths=**} {
      allow read;
      // Permite la escritura (subir, actualizar, eliminar) solo a usuarios autenticados
      allow write: if request.auth != null;
    }
  }
}
